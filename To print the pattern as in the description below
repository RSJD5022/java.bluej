public class java {
    public static void main() {
        for (int i = 1; i <= 5; i++) {
            for (int j = 1; j <= 5; j++) {
                if (i==j)
                    System.out.print(j + "  ");
                else
                    System.out.print("*  ");
            }System.out.println();
        }
        
    }
}
